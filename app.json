{
  "expo": {
    "name": "moodlog",
    "slug": "moodlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/images/icon.png",
    "newArchEnabled": true,
    "scheme": "moodlog",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kidboi666.moodlog"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./public/images/icon.png",
          "dark": {
            "image": "./public/images/splash-dark.png",
            "backgroundColor": "#232323"
          },
          "resizeMode": "contain"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1dcbc0cb-0ee7-40f3-901b-69323e720822"
      }
    }
  }
}
